import ros2_ui.web_ui.app


def main():
    """
    Starts up the web GUI by running the main function in ros2_ui.web_ui.app.
    :return:
    """
    ros2_ui.web_ui.app.main()


if __name__ == "__main__":
    try:
        # ros2_ui.startup.main(sys.argv[1:])
        main()
    except KeyboardInterrupt:
        exit("\n\nKilled via KeyboardInterrupt (Ctrl+C). ByeBye!")
    print("Thanks for using. ByeBye!")
