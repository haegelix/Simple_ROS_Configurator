#!/bin/sh

######################################################
#  This is just a wrapper for the real application.  #
#  It will simply call it and exit when it is done.  #
#           Nothing more, nothing less.              #
######################################################

# TODO delete dev environment
export FLASK_ENV=development

# source ros2 foxy environment
. /opt/ros/foxy/setup.sh

# startup my app
python3 /usr/local/lib/srosc/srosc.py "$@"

# exit using the exit code of above instruction
exit $?